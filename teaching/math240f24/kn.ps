%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 230 40
%%HiResBoundingBox: 0 0 230 40

% Constants
/cm {28.35 mul} def
/u {0.5 cm mul} def     % One unit
/w {0.5 cm mul} def
/xoff {0.1 cm add} def   % Offset the letters
/yoff {0.1 cm add} def

/nine {/CMMI10 findfont 9 scalefont setfont} def
/ninetext {/CMR10 findfont 9 scalefont setfont} def
/sixtext {/CMR8 findfont 6 scalefont setfont} def
/symnine {/CMSY10 findfont 9 scalefont setfont} def

0.5 setlinewidth

% k n deg:  get degrees of k/n of the way around unit circle
/deg {
    /n exch def
    /k exch def
    360 k mul n div
} def

/dot {
    2 0 360 arc closepath fill
} def

/s 20 string def
/label {
    /n exch def
    ninetext n s cvs show
} def

% K1
gsave
1 u 0 w translate
1 1 deg sin u 1 1 deg cos w dot
grestore
%K2
gsave
3 u 1 w translate
1 2 deg sin u 1 2 deg cos w dot
2 2 deg sin u 2 2 deg cos w dot
newpath
1 2 deg sin u 1 2 deg cos w moveto
2 2 deg sin u 2 2 deg cos w lineto
stroke
grestore
