%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 60 60
%%HiResBoundingBox: 0 0 60 60

% Constants
/cm {28.35 mul} def
/u {2 cm mul} def     % One unit
/w {2 cm mul} def
/xoff {0.1 cm add} def   % Offset the letters
/yoff {0.1 cm add} def

/nine {/CMMI10 findfont 9 scalefont setfont} def
/ninetext {/CMR10 findfont 9 scalefont setfont} def
/sixtext {/CMR8 findfont 6 scalefont setfont} def
/symnine {/CMSY10 findfont 9 scalefont setfont} def

0.5 setlinewidth

/dot {
    2 0 360 arc closepath fill
} def

/s 20 string def
/label {
    /n exch def
    ninetext n s cvs show
} def

% L1
gsave
0 u 0 w dot
0 u 1 w dot
1 u 1 w dot
1 u 0 w dot
0 u 1 w moveto
-8 0 rmoveto 1 label
1 u 1 w moveto
4 0 rmoveto 2 label
0 u 0 w moveto
-8 -7 rmoveto 3 label
1 u 0 w moveto
4 -7 rmoveto 4 label
newpath
0 u 0 w moveto
0 u 1 w lineto
1 u 1 w lineto
0 u 1 w moveto
1 u 0 w lineto
1 u 1 w lineto
stroke
grestore

showpage
