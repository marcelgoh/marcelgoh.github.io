%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 60 60
%%HiResBoundingBox: 0 0 60 60

% Constants
/cm {28.35 mul} def
/u {1 cm mul} def     % One unit
/w {1 cm mul} def
/xoff {0.1 cm add} def   % Offset the letters
/yoff {0.1 cm add} def

/nine {/CMMI10 findfont 9 scalefont setfont} def
/ninetext {/CMR10 findfont 9 scalefont setfont} def
/sixtext {/CMR8 findfont 6 scalefont setfont} def
/symnine {/CMSY10 findfont 9 scalefont setfont} def

0.5 setlinewidth

% k n deg:  get degrees of k/n of the way around unit circle
/deg {
    /n exch def
    /k exch def
    360 k mul n div
} def

/dot {
    2 0 360 arc closepath fill
} def

/s 20 string def
/label {
    /n exch def
    ninetext n s cvs show
} def

% L1
gsave
1 u 1 w translate
1 5 deg sin u 1 5 deg cos w dot
2 5 deg sin u 2 5 deg cos w dot
3 5 deg sin u 3 5 deg cos w dot
4 5 deg sin u 4 5 deg cos w dot
5 5 deg sin u 5 5 deg cos w dot
newpath
1 5 deg sin u 2 add 1 5 deg cos w 2 add moveto 5 label
2 5 deg sin u 3 add 2 5 deg cos w -5 add moveto 4 label
3 5 deg sin u -7 add 3 5 deg cos w -5 add moveto 3 label
4 5 deg sin u -7 add 4 5 deg cos w 2 add moveto 2 label
5 5 deg sin u 1 add 5 5 deg cos w 2 add moveto 1 label
2 5 deg sin u 2 5 deg cos w moveto
4 5 deg sin u 4 5 deg cos w lineto
5 5 deg sin u 5 5 deg cos w lineto
1 5 deg sin u 1 5 deg cos w lineto
3 5 deg sin u 3 5 deg cos w lineto
stroke
grestore

showpage
