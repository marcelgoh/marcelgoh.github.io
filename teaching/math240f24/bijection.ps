%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 290 90
%%HiResBoundingBox: 0 0 290 90

% Constants
/cm {28.35 mul} def
/u {0.4 cm mul} def     % One unit
/w {0.4 cm mul} def
/xoff {0.1 cm add} def   % Offset the letters
/yoff {0.1 cm add} def

/nine {/CMMI10 findfont 9 scalefont setfont} def
/ninetext {/CMR10 findfont 9 scalefont setfont} def
/sixtext {/CMR8 findfont 6 scalefont setfont} def
/symnine {/CMSY10 findfont 9 scalefont setfont} def

0.5 setlinewidth

% k n deg:  get degrees of k/n of the way around unit circle
/deg {
    /n exch def
    /k exch def
    360 k mul n div
} def

/arrowhead {  % x y size theta
    /theta exch def
    /s exch def
    /y exch def
    /x exch def
    /h s 2 div def
    gsave
        x y translate
        theta rotate
        newpath
            0 0 moveto
            h 0 s h s s rcurveto
            s neg s neg rmoveto
            h 0 s h neg s s neg rcurveto
        stroke
    grestore
} def

/dot {
    2 0 360 arc closepath fill
} def

/s 20 string def
/label {
    /n exch def
    ninetext n s cvs show
} def

gsave
0 u 0.2 w translate
